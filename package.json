  },
  "homepage": "https://github.com/perdevpernull/myinfo#readme",
  "dependencies": {
    "express": "^4.16.3"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^4.19.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3"
  }
