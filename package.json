{
  "name": "myinfo",
  "version": "0.0.1",
  "description": "MyInfo: Just another mindmap application.",
  "main": "myinfoServer.js",
  "scripts": {
    "build": "run-s build_myinfo build_lp_thebrain build_dp_note",
    "build_myinfo": "rollup -c myinfo.rollup.config.js",
    "build_lp_thebrain": "rollup -c lp_thebrain.rollup.config.js",
    "build_dp_note": "rollup -c dp_note.rollup.config.js",
    "watch": "rollup -c myinfo.rollup.config.js -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "nodemon myinfoServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perdevpernull/myinfo.git"
  },
  "keywords": [
    "mindmap",
    "thebrain"
  ],
  "author": "\\dev\\null",
  "contributors": [
    {
      "name": "Szil",
      "email": "nemes.gergo@protonmail.com"
    }
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/perdevpernull/myinfo/issues"
  },
  "homepage": "https://github.com/perdevpernull/myinfo#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "livereload": "^0.7.0",
    "memorystore": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^4.13.0",
    "nodemon": "^1.17.4",
    "npm-run-all": "^4.1.2",
    "rollup": "^0.59.0",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
