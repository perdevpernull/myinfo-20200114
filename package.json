{
  "name": "myinfo",
  "version": "0.0.1",
  "description": "MyInfo: Just another mindmap application.",
  "main": "myinfoServer.js",
  "scripts": {
    "build": "run-s clean build_myinfo deploy_myinfo build_lp_thebrain deploy_lp_thebrain build_dp_note deploy_dp_note",
    "clean": "del build\\*.js",
    "build_myinfo": "rollup -c myinfo.rollup.config.js",
    "deploy_myinfo": "copy /Y build\\myinfo.js public\\myinfo.js",
    "build_lp_thebrain": "rollup -c lp_thebrain.rollup.config.js",
    "deploy_lp_thebrain": "copy /Y build\\lp_thebrain.js public\\lp_thebrain.js",
    "build_dp_note": "rollup -c dp_note.rollup.config.js",
    "deploy_dp_note": "copy /Y build\\dp_note.js public\\dp_note.js",
    "start": "start node myinfoServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perdevpernull/myinfo.git"
  },
  "keywords": [
    "mindmap",
    "thebrain"
  ],
  "author": "\\dev\\null",
  "contributors": [
    {
      "name": "Szil",
      "email": "nemes.gergo@protonmail.com"
    }
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/perdevpernull/myinfo/issues"
  },
  "homepage": "https://github.com/perdevpernull/myinfo#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "jshint": "^2.9.5"
  }
}
